<div class="row step">
    <div class="large-12 columns">
        <h5 class="subhearder">Retiro contenedor vacio</h5>
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Region</label>
        {{view Ember.Select content=names}}
    </div>
    <div class="large-6 columns">
        <label>Comuna</label>
        {{view Ember.Select content=names}}
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Fecha</label>
        {{view Ember.Select content=names}}
    </div>
    <div class="large-6 columns">
        <label>Horario</label>
        {{view Ember.Select content=names}}
    </div>
</div>

<div class="row">
    <div class="large-12 columns">
        <label>Razon social</label>
        {{input type="text" value=razonName  placeholder=""}}
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Rut empresa</label>
        {{input type="text" value=rut  placeholder="Sin puntos ni guion"}}
    </div>
    <div class="large-6 columns">
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Dirección</label>
        {{input type="text" value=address  placeholder=""}}
    </div>
    <div class="large-3 columns">
        <label>Numero</label>
        {{input type="text" value=addressNumber  placeholder=""}}
    </div>
    <div class="large-3 columns">
        <label>Oficina</label>
        {{input type="text" value=addressOffice  placeholder=""}}
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Comuna</label>
        {{input type="text" value=commune  placeholder=""}}
    </div>
    <div class="large-6 columns">
        <label>Ciudad</label>
        {{input type="text" value=city  placeholder=""}}
    </div>
</div>

<div class="row">
    <div class="large-6 columns">
        <label>Región</label>
        {{input type="text" value=region  placeholder=""}}
    </div>
    <div class="large-6 columns">
    </div>
</div>

<div class="row truck-number">
    <div class="large-12 columns">
        <label>¿Tiene camiones propios?</label>
        <ul class="radiobuttons truck">
            <li class="awnser-yes"><input type="radio" id="a1" name="2" value="1"> <label for="a1">Si</label></li>
            <li class="awnser-no"><input type="radio" id="a2" name="2" value="2"> <label for="a2">No</label></li>
        </ul>
    </div>
</div>

<div class="row awnser truck">
    <div class="large-6 columns">
        <label>Numero de camiones</label>
        {{input type="text" value=truckNumber  placeholder=""}}
    </div>
    <div class="large-6 columns">
    </div>
</div>

<div class="row">
    <div class="large-12 columns">
        <label>¿Cuenta con almacenaje?</label>
        <ul class="radiobuttons storage">
            <li class="awnser-yes"><input type="radio" id="b1" name="2" value="1"> <label for="b1">Si</label></li>
            <li class="awnser-no"><input type="radio" id="b2" name="2" value="2"> <label for="b2">No</label></li>
        </ul>
    </div>
</div>

<div class="row awnser storage">
    <div class="large-6 columns">
        <label>Tipo de almacenaje</label>
        {{input type="text" value=storageType  placeholder="Cubierto, Rack, etc…"}}
    </div>
    <div class="large-6 columns">
        <label>Capacidad</label>
        {{input type="text" value=storageCapacity  placeholder="m2"}}
    </div>
</div>

{{! end datos personales}}

